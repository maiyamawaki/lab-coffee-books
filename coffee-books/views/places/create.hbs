<h1>Add new one</h1>

<div id="map" style="width: 100%; height: 400px"></div>
<br>
<br>

<form action="/places/create" method="POST"

<label for="name">Name : </label>
<input type="text" name="name" placeholder="Name">
<br>

<label for="placeType">PlaceType</label>
<input type="text" name="placeType" placeholder="placeType">
<br>

<label for="lng">For lng</label>
<input type="text" name="lng" id="lng">

<label for="lat">For lat</label>
<input type="text" name="lat" id="lat">
<br>
<button type="submit">Create!</button>

</form>

<br>
<br>
<a href="/places/home">Back</a>

<script src="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-geocoder/v4.5.1/mapbox-gl-geocoder.min.js"></script>
